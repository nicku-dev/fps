        <record id="payment_requisition_sequence" model="ir.sequence">
            <field name="name">Payment Requisition Sequence</field>
            <field name="code">payment.requisition.pengajuan</field>
            <field name="prefix">PPJ/%(year)s/%(month)s/%(day)s-</field>
